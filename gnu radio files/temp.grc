- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\nimport pmt\n\nclass\
      \ TagSwitch(gr.sync_block):\n    def __init__(self,tag_key=\"\",button=1):\n\
      \        gr.sync_block.__init__(self,\n                               name=\"\
      TagSwitch\",\n                               in_sig=[np.int8],\n           \
      \                    out_sig=[np.int8]*2)\n        self.tag_key = tag_key\n\
      \        self.current_output = 0\n        self.button=button\n\n    def work(self,\
      \ input_items, output_items):\n        tags = self.get_tags_in_range(0, 0, len(input_items[0]))\n\
      \        for tag in tags:\n            if pmt.to_python(tag.key) == self.tag_key:\n\
      \                self.current_output = 1 if self.current_output == 0 else 0\n\
      \                print(self.current_output)\n        \n        out_data_1 =\
      \ output_items[1]\n        out_data_0 = output_items[0]\n\n        in_data =\
      \ input_items[0]\n\n        if(int(self.current_output)):in_data = input_items[0]*0\n\
      \        else: pass\n        \n\n        # Copy input to both outputs\n    \
      \    out_data_0[:] = in_data*self.button\n        out_data_1[:] = in_data*self.button\n\
      \n        return len(in_data)\n        "
    affinity: ''
    alias: ''
    button: i
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
    tag_key: '''switch'''
  states:
    _io_cache: ('TagSwitch', 'TagSwitch', [('tag_key', "''"), ('button', '1')], [('0',
      'byte', 1)], [('0', 'byte', 1), ('1', 'byte', 1)], '', ['button', 'tag_key'])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1080, 372.0]
    rotation: 0
    state: enabled
- name: epy_block_1
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\n\nclass bit_stream_generator(gr.sync_block):\n\
      \    \"\"\"\n    Custom block to generate a bit stream.\n    \"\"\"\n    def\
      \ __init__(self, bit_pattern=\"101010\", samples_per_bit=1,sample_rate=32000):\n\
      \        \"\"\"\n        Initialize the block.\n        :param bit_pattern:\
      \ String of '1's and '0's representing the bit pattern.\n        :param bit_rate:\
      \ Number of bits per second.\n        \"\"\"\n        self.bit_pattern = [int(b)\
      \ for b in bit_pattern]\n        #self.bit_rate = bit_rate\n        self.sample_rate\
      \ = int(sample_rate)  # Default sample rate in samples/sec.\n        self.samples_per_bit\
      \ = int(samples_per_bit)\n        self.noofcycles=10\n        self.currentcycle=0\n\
      \        \n        super().__init__(\n            name=\"bit_stream_generator\"\
      ,\n            in_sig=None,\n            out_sig=[np.int8]  # Output a stream\
      \ of bits as 8-bit integers\n        )\n\n    def work(self, input_items, output_items):\n\
      \        \"\"\"\n        Process block logic.\n        \"\"\"\n        output\
      \ = output_items[0]\n        #if(self.currentcycle<self.noofcycles):\n     \
      \   num_bits = len(output) // self.samples_per_bit\n        bit_sequence = np.tile(self.bit_pattern,\
      \ num_bits // len(self.bit_pattern) + 1)#divition only put \n        output[::]\
      \ = bit_sequence[::]\n        return len(output)\n"
    affinity: ''
    alias: ''
    bit_pattern: '""'
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
    sample_rate: '100'
    samples_per_bit: '1'
  states:
    _io_cache: ('bit_stream_generator', 'bit_stream_generator', [('bit_pattern', "'101010'"),
      ('samples_per_bit', '1'), ('sample_rate', '32000')], [], [('0', 'byte', 1)],
      '\n    Custom block to generate a bit stream.\n    ', ['bit_pattern', 'sample_rate',
      'samples_per_bit'])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [128, 384.0]
    rotation: 0
    state: enabled
